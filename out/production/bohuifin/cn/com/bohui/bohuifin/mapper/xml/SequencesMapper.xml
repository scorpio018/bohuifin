<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.SequencesMapper">	<resultMap type="SequencesBean" id="SequencesResult">		<result column="seq_name" property="seqName"/>		<result column="init_val" property="initVal"/>		<result column="max_val" property="maxVal"/>		<result column="cur_val" property="curVal"/>		<result column="step_val" property="stepVal"/>		<result column="cache_size" property="cacheSize"/>		<result column="seq_memo" property="seqMemo"/>	</resultMap>	<sql id="baseSelect">		seq_name,		init_val,		max_val,		cur_val,		step_val,		cache_size,		seq_memo	</sql>	<insert id="saveSequences" parameterType="SequencesBean">		insert into t_sequences(seq_name, init_val, max_val, cur_val, step_val, cache_size, seq_memo)		values (#{seqName}, #{initVal}, #{maxVal}, #{curVal}, #{stepVal}, #{cacheSize}, #{seqMemo})	</insert>	<update id="updateSequences" parameterType="SequencesBean">		update t_sequences		set			init_val = #{initVal},			max_val = #{maxVal},			cur_val = #{curVal},			step_val = #{stepVal},			cache_size = #{cacheSize},			seq_memo = #{seqMemo}		where			seq_name = #{seqName}	</update>	<select id="listSequencesByPage" parameterType="page" resultMap="SequencesResult">		select			<include refid="baseSelect"/>		from			t_sequences	</select>	<delete id="removeSequencesById" parameterType="int">		delete from t_sequences where seq_name = #{seqName}	</delete>	<!-- 查询多条记录(按照用户名或者真实姓名模糊查询) edit By cn-->	<select id="selectCurValBySeqName" parameterType="String" resultType="long" >		select nextval(#{seqName}) as c	</select>	<!-- 查询所有记录 -->	<update id="updateBySeqName" parameterType="String">		update t_sequences seq set seq.cur_val=seq.cur_val+seq.step_val where seq_name=#{seqName}	</update>	<insert id="insertSeq" parameterType="hashmap">		insert into t_sequences (seq_name,init_val,cur_val) values (#{seqName},#{initValue},#{curValue})	</insert>	<resultMap type="SequencesBean" id="seq-result">		<id property="seqName" column="seq_name" />		<result property="initVal" column="init_val" />		<result property="maxVal" column="max_val" />		<result property="curVal" column="cur_val" />		<result property="stepVal" column="step_val" />		<result property="cacheSize" column="cache_size" />		<result property="seqMemo" column="seq_memo" />	</resultMap></mapper>