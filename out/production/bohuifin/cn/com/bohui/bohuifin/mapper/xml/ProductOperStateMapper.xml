<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.ProductOperStateMapper">    <resultMap type="ProductOperStateBean" id="ProductOperStateResult">        <result column="product_id" property="productId"/>        <result column="oper_state_id" property="operStateId"/>        <result column="oper_state_start_time" property="operStateStartTime"/>        <result column="list_order" property="listOrder"/>        <result column="create_time" property="createTime"/>        <result column="create_by" property="createBy"/>        <result column="update_time" property="updateTime"/>        <result column="update_by" property="updateBy"/>        <result column="state" property="state"/>    </resultMap>    <sql id="baseSelect">		product_id,		oper_state_id,		oper_state_start_time,		list_order,		create_time,		create_by,		update_time,		update_by,		state	</sql>    <insert id="saveProductOperState" parameterType="ProductOperStateBean">		insert into t_product_oper_state(product_id, oper_state_id, oper_state_start_time, list_order, create_time, create_by, update_time, update_by, state)		values (#{productId}, #{operStateId}, #{operStateStartTime}, #{listOrder}, #{createTime}, #{createBy}, #{updateTime}, #{updateBy}, #{state})	</insert>    <update id="updateProductOperState" parameterType="ProductOperStateBean">        update t_product_oper_state        set        oper_state_start_time = #{operStateStartTime}        <if test="listOrder != null">            ,list_order = #{listOrder}        </if>        <if test="updateTime != null">            ,update_time = #{updateTime}        </if>        <if test="updateBy != null">            ,update_by = #{updateBy}        </if>        <if test="state != null">            ,state = #{state}        </if>        where        product_id = #{productId}        and        oper_state_id = #{operStateId}    </update>    <select id="listProductOperStateByPage" parameterType="page" resultMap="ProductOperStateResult">        select        <include refid="baseSelect"/>        from        t_product_oper_state    </select>    <select id="listProductOperStateByProductId" parameterType="java.util.Map" resultMap="ProductOperStateResult">        select        <include refid="baseSelect"/>        from        t_product_oper_state        where        product_id = #{productId}        and        state = #{state}    </select>    <select id="findProductOperStateByProductAndOperState" parameterType="java.util.Map"            resultMap="ProductOperStateResult">        select        <include refid="baseSelect"/>        from        t_product_oper_state        where        product_id = #{productId}        and        oper_state_id = #{operStateId}        and        state = #{state}    </select>    <delete id="removeProductOperStateById" parameterType="int">		delete from t_product_oper_state where product_id = #{productId}	</delete></mapper>