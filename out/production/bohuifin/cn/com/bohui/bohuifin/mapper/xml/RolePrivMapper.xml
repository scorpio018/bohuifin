<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.RolePrivMapper">	<resultMap type="RolePrivBean" id="RolePrivResult">		<result column="role_id" property="roleId"/>		<result column="priv_ename" property="privEname"/>	</resultMap>	<sql id="baseSelect">		role_id,		priv_ename	</sql>	<insert id="saveRolePriv" parameterType="RolePrivBean">		insert into t_role_priv(role_id, priv_ename)		values (#{roleId}, #{privEname})	</insert>	<update id="updateRolePriv" parameterType="RolePrivBean">		update t_role_priv		set			priv_ename = #{privEname}		where			role_id = #{roleId}	</update>	<select id="listRolePrivByPage" parameterType="page" resultMap="RolePrivResult">		select			<include refid="baseSelect"/>		from			t_role_priv	</select>	<delete id="removeRolePrivById" parameterType="int">		delete from t_role_priv where role_id = #{roleId}	</delete></mapper>