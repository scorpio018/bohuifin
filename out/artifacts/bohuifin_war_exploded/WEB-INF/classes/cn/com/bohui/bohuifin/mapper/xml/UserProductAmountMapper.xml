<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.UserProductAmountMapper">	<resultMap type="UserProductAmountBean" id="UserProductAmountResult">		<result column="user_id" property="userId"/>		<result column="product_id" property="productId"/>		<result column="amount" property="amount"/>	</resultMap>	<sql id="baseSelect">		user_id,		product_id,		amount	</sql>	<insert id="saveUserProductAmount" parameterType="UserProductAmountBean">		insert into t_user_product_amount(user_id, product_id, amount)		values (#{userId}, #{productId}, #{amount})	</insert>	<update id="updateUserProductAmount" parameterType="UserProductAmountBean">		update t_user_product_amount		set			product_id = #{productId}			<if test="amount != null">				,amount = #{amount}			</if>		where			user_id = #{userId}	</update>	<select id="listUserProductAmountByPage" parameterType="page" resultMap="UserProductAmountResult">		select			<include refid="baseSelect"/>		from			t_user_product_amount	</select>	<delete id="removeUserProductAmountById" parameterType="UserProductAmountBean">		delete from t_user_product_amount where user_id = #{userId}	</delete></mapper>