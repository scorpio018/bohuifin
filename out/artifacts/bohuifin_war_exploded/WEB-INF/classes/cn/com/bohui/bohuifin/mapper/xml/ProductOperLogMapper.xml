<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.ProductOperLogMapper">	<resultMap type="ProductOperLogBean" id="ProductOperLogResult">		<result column="oper_id" property="operId"/>		<result column="product_id" property="productId"/>		<result column="oper_state_id" property="operStateId"/>		<result column="create_time" property="createTime"/>		<result column="create_by" property="createBy"/>		<result column="state" property="state"/>	</resultMap>	<sql id="baseSelect">		oper_id,		product_id,		oper_state_id,		create_time,		create_by,		state	</sql>	<insert id="saveProductOperLog" parameterType="ProductOperLogBean">		insert into t_product_oper_log(oper_id, product_id, oper_state_id, create_time, create_by, state)		values (#{operId}, #{productId}, #{operStateId}, #{createTime}, #{createBy}, #{state})	</insert>	<update id="updateProductOperLog" parameterType="ProductOperLogBean">		update t_product_oper_log		set			product_id = #{productId},			oper_state_id = #{operStateId},			create_time = #{createTime},			create_by = #{createBy},			state = #{state}		where			oper_id = #{operId}	</update>	<select id="listProductOperLogByPage" parameterType="page" resultMap="ProductOperLogResult">		select			<include refid="baseSelect"/>		from			t_product_oper_log	</select>	<delete id="removeProductOperLogById" parameterType="int">		delete from t_product_oper_log where oper_id = #{operId}	</delete></mapper>