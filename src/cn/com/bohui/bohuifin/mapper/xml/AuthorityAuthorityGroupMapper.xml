<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.AuthorityAuthorityGroupMapper">	<resultMap type="AuthorityAuthorityGroupBean" id="AuthorityAuthorityGroupResult">		<result column="authority_id" property="authorityId"/>		<result column="authority_group_id" property="authorityGroupId"/>	</resultMap>	<sql id="baseSelect">		authority_id,		authority_group_id	</sql>	<insert id="saveAuthorityAuthorityGroup" parameterType="AuthorityAuthorityGroupBean">		insert into t_authority_authority_group(authority_id, authority_group_id)		values (#{authorityId}, #{authorityGroupId})	</insert>	<update id="updateAuthorityAuthorityGroup" parameterType="AuthorityAuthorityGroupBean">		update t_authority_authority_group		set			authority_group_id = #{authorityGroupId}		where			authority_id = #{authorityId}	</update>	<select id="listAuthorityAuthorityGroupByPage" parameterType="page" resultMap="AuthorityAuthorityGroupResult">		select			<include refid="baseSelect"/>		from			t_authority_authority_group	</select>	<delete id="removeAuthorityAuthorityGroupById" parameterType="int">		delete from t_authority_authority_group where authority_id = #{authorityId}	</delete></mapper>