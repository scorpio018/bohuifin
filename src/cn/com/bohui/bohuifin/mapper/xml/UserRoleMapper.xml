<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.UserRoleMapper">	<resultMap type="UserRoleBean" id="UserRoleResult">		<result column="user_id" property="userId"/>		<result column="dept_id" property="deptId"/>		<result column="role_id" property="roleId"/>	</resultMap>	<sql id="baseSelect">		user_id,		dept_id,		role_id	</sql>	<insert id="saveUserRole" parameterType="UserRoleBean">		insert into t_user_role(user_id, dept_id, role_id)		values (#{userId}, #{deptId}, #{roleId})	</insert>	<update id="updateUserRole" parameterType="UserRoleBean">		update t_user_role		set			dept_id = #{deptId},			role_id = #{roleId}		where			user_id = #{userId}	</update>	<select id="listUserRoleByPage" parameterType="page" resultMap="UserRoleResult">		select			<include refid="baseSelect"/>		from			t_user_role	</select>	<delete id="removeUserRoleById" parameterType="int">		delete from t_user_role where user_id = #{userId}	</delete></mapper>