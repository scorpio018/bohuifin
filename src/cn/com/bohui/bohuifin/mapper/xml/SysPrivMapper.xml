<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.SysPrivMapper">	<resultMap type="SysPrivBean" id="SysPrivResult">		<result column="priv_ename" property="privEname"/>		<result column="priv_cname" property="privCname"/>		<result column="priv_desc" property="privDesc"/>		<result column="list_order" property="listOrder"/>		<result column="state" property="state"/>	</resultMap>	<sql id="baseSelect">		priv_ename,		priv_cname,		priv_desc,		list_order,		state	</sql>	<insert id="saveSysPriv" parameterType="SysPrivBean">		insert into t_sys_priv(priv_ename, priv_cname, priv_desc, list_order, state)		values (#{privEname}, #{privCname}, #{privDesc}, #{listOrder}, #{state})	</insert>	<update id="updateSysPriv" parameterType="SysPrivBean">		update t_sys_priv		set			priv_cname = #{privCname},			priv_desc = #{privDesc},			list_order = #{listOrder},			state = #{state}		where			priv_ename = #{privEname}	</update>	<select id="listSysPrivByPage" parameterType="page" resultMap="SysPrivResult">		select			<include refid="baseSelect"/>		from			t_sys_priv	</select>	<delete id="removeSysPrivById" parameterType="int">		delete from t_sys_priv where priv_ename = #{privEname}	</delete></mapper>