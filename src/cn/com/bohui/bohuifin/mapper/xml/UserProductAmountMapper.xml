<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.com.bohui.bohuifin.mapper.UserProductAmountMapper">	<resultMap type="UserProductAmountBean" id="UserProductAmountResult">		<result column="amount_id" property="amountId"/>		<result column="user_id" property="userId"/>		<result column="product_id" property="productId"/>		<result column="amount" property="amount"/>		<result column="invest_time" property="investTime"/>		<result column="state" property="state"/>		<result column="create_time" property="createTime"/>		<result column="update_time" property="updateTime"/>	</resultMap>	<sql id="baseSelect">		amount_id,		user_id,		product_id,		amount,		invest_time,		state,		create_time,		update_time	</sql>	<insert id="saveUserProductAmount" parameterType="UserProductAmountBean">		insert into t_user_product_amount(amount_id, user_id, product_id, amount, invest_time, state, create_time, update_time)		values (#{amountId}, #{userId}, #{productId}, #{amount}, #{investTime}, #{state}, #{createTime}, #{updateTime})	</insert>	<select id="listUserProductAmountByPage" parameterType="page" resultMap="UserProductAmountResult">		select			<include refid="baseSelect"/>		from			t_user_product_amount	</select>	<select id="listUserProductAmountByUser" parameterType="java.lang.String" resultMap="UserProductAmountResult">		select			<include refid="baseSelect"/>		from			t_user_product_amount		where			user_id = #{userId}	</select>	<select id="listUserProductAmountByProduct" parameterType="java.lang.String" resultMap="UserProductAmountResult">	</select>	<update id="removeUserProductAmountById" parameterType="UserProductAmountBean">		update		t_user_product_amount		set		state = #{state}		where		amount_id = #{amountId}	</update></mapper>